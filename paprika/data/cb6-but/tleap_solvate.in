source leaprc.protein.ff14SB
source leaprc.water.tip3p
source leaprc.gaff

# These paths are so this file can be run from inside ../../tests/tmp directory.

loadamberparams ../../data/cb6-but/cb6.frcmod
CB6 = loadmol2 ../../data/cb6-but/cb6.mol2
loadamberparams ../../data/cb6-but/but.frcmod
BUT = loadmol2 ../../data/cb6-but/but.mol2

model = loadpdb ../../data/cb6-but/cb6-but.pdb

check model
savepdb model vac.pdb
saveamberparm model vac.topo vac.crds

solvatebox model TIP3PBOX {10.0 10.0 10.0}
addionsrand model Na 0
addionsrand model Cl 0

savepdb model full.pdb
saveamberparm model full.topo full.crds

desc model

quit
