source leaprc.protein.ff14SB
source leaprc.gaff
source leaprc.water.tip3p
loadamberparams sac.frcmod
SAC = loadmol2 sac.mol2

model = loadpdb dock-prep-xray7-wat-vac2.pdb

check model
savepdb model vac.pdb
saveamberparm model vac.topo vac.crds

solvateoct model TIP3PBOX 11.84 iso
addionsrand model Na+ 0
addionsrand model Cl- 0
addionsrand model Na+ 75
addionsrand model Cl- 75

#BeginRemoveStuff
remove model model.28767

remove model model.28766
remove model model.28765
remove model model.28764
remove model model.28763
remove model model.28762
remove model model.28761
#EndRemoveStuff

savepdb model full.pdb
saveamberparm model full.topo full.crds

desc model

quit

